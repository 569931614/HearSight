# HearSight Server Configuration

# Server
PORT=9999
STATIC_DIR=app_datas/download_videos

# PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/hearsight
# Or use individual variables:
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=postgres
# POSTGRES_DB=hearsight

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Embedding API (SiliconFlow or compatible)
QDRANT_EMBEDDING_API_URL=https://api.siliconflow.cn/v1
QDRANT_EMBEDDING_API_KEY=your_siliconflow_api_key
QDRANT_EMBEDDING_MODEL=BAAI/bge-large-zh-v1.5

# LLM API (OpenAI compatible)
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_CHAT_MODEL=gpt-4o-mini

# RAG Settings
RAG_ENABLED=true
RAG_INCLUDE_SUMMARIES=true

# JWT Authentication
JWT_SECRET=your-secret-key-change-in-production

# Aliyun OSS (Optional)
OSS_ENABLED=false
OSS_ACCESS_KEY_ID=
OSS_ACCESS_KEY_SECRET=
OSS_BUCKET=
OSS_ENDPOINT=
OSS_REGION=oss-cn-hangzhou

# CORS
ALLOW_ORIGINS=http://localhost:5173,http://localhost:10000
